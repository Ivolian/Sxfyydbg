                        android:layout_marginTop="5dp"
                        android:paddingLeft="5dp"
                        android:text="会议室申请" />
                </LinearLayout>
            </FrameLayout>
             <FrameLayout
                android:id="@+id/fra_nbfw"
                style="@style/main_frame_layout_bg"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:background="@drawable/main_nbfw_bg" >

                <LinearLayout
                    style="@style/main_frame1_linear_layout_bg"
                    android:layout_gravity="center_horizontal|center_vertical" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:src="@drawable/nbfw_log" />

                    <TextView
                        style="@style/main_tv_bg"
                        android:layout_marginTop="10dp"
                        android:text="内部发文" />
                </LinearLayout>
            </FrameLayout>
             <FrameLayout
                android:id="@+id/fra_xwzx"
                style="@style/main_frame_layout_bg"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:background="@drawable/main_xwzx_bg" >

                <LinearLayout
                    style="@style/main_frame1_linear_layout_bg"
                    android:layout_gravity="center_horizontal|center_vertical" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:src="@drawable/gdmz" />

                    <TextView
                        style="@style/main_tv_bg"
                        android:layout_marginTop="10dp"
                        android:text="新闻中心" />
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>

