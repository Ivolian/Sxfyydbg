<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="PlanDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="text_style">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="edittext_style">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">15sp</item>
        <item name="android:singleLine">false</item>
        <item name="android:scrollbars">vertical</item>
    </style>
    <style name="login_tv_bottom_bg">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height" >wrap_content</item>
        <item name="android:textColor">#C9D0D5</item>
        <item name="android:textSize">17sp</item>
        <item name="android:background">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
    <style name="login_tv_top_bg">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height" >wrap_content</item>
        <item name="android:textColor">#C9D0D5</item>
        <item name="android:textSize">20sp</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
    <style name="main_linear_layout_bg">
        <item name="android:layout_width" >match_parent</item>
        <item name="android:layout_height" >110dp</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="main_frame_layout_bg">
        <item name="android:layout_width" >0dp</item>
        <item name="android:layout_height" >match_parent</item>
    </style>
    <style name="main_frame1_linear_layout_bg">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height" >wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="main_tv_bg">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height" >wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="rela_zwsj_bg">
        <item name="android:layout_width" >match_parent</item>
        <item name="android:layout_height" >wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_margin">20dp</item>
        <item name="android:background">@drawable/white_bg</item>
    </style>
    <style name="iv_zwsj_bg">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height" >wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">20dp</item>
